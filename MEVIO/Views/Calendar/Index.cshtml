        //dayBtn.addEventListener("click", DayButtonClick);
        //weekBtn.addEventListener("click", WeekButtonClick);
        monthBtn.addEventListener("click", CurrentMonth);
        //yearBtn.addEventListener("click", CurrentMonth);

        function CurrentMonth() {
            var currentMonth = @ViewBag.monthNow.Number;
            var monthSwitchers = document.querySelectorAll('.month-switcher');
            //var currentMonth = new Date().getMonth() + 1;
            var currentMonth = @ViewBag.monthNow.Number;
            var monthSwitchers = document.querySelectorAll('.month-switcher');

            for (var i = 0; i < monthSwitchers.length; i++) {
                var monthSwitcher = monthSwitchers[i];
                var monthName = monthSwitcher.classList[1].split('-')[0];
                var monthNumber = new Date(Date.parse('01 ' + monthName + ' 2023')).getMonth() + 1;

                if (monthNumber !== currentMonth) {
                    monthSwitcher.style.display = 'none';
                }
                else {
                    monthSwitcher.style.display = 'block';
                }
            }
        }
    </script>
    <script>
        function toggleVisibility(id) {
            var allYears = document.querySelectorAll('.year-switcher');
            for (var i = 0; i < allYears.length; i++) {
                if (allYears[i].id === id) {
                    allYears[i].style.display = 'block';
                } else {
                    allYears[i].style.display = 'none';
