    <div id="monthGrid" class="calendar-grid calGrid">
        @for (int year = 2020; year <= 2026; year++)
        {
            <div class="year-switcher" id="@($"year_{year}")" @(year == 2020 ? "style='display:block'" : "style='display:none'")>
                @for (int month = 1; month <= 12; month++)
                {
                    var monthName = DateTimeFormatInfo.CurrentInfo.GetMonthName(month);

                    <div class="month-switcher @monthName">
                        <div class="calendar-header">
                            <div class="month-name pointer">@monthName @year</div>@*@ViewBag.monthNames[month-1]*@
                            <div class="select-month">
                                <label class="label pointer">Місяць</label>
                                <img class="arrow pointer" src="~/img/keyboard-left-arrow-button_icon-icons 5.png" alt="">
                                <div class="options-month hide">
                                    @for (int m = 1; m <= 12; m++)
                                    {
                                        //here
                                        <div class="option-item option-item-month">
                                            @DateTimeFormatInfo.CurrentInfo.GetMonthName(m)
                                        </div>
                                    }
                                </div>
                            </div>
                            <div class="select-years">
                                <label class="label pointer">Рік</label>
                                <img class="arrow pointer" src="~/img/keyboard-left-arrow-button_icon-icons 5.png" alt="">
                                <div class="options-years hide">
                                    @for (int y = 2020; y <= 2025; y++)
                                    {
                                        <div class="option-item" onclick="toggleVisibility('@($"year_{y}")'); setMyValue('@y');">@y</div>
                                    }
                                </div>
                            </div>
                            <input type="hidden" name="yearValue" id="myInput">
                        </div>

                        <div>
                            <div class="grid">
                                @for (int i = 0; i < 7; i++)
                                {
                                    <div class="week-day">@ViewBag.weekDays[i]@*@dayOfWeek.ToString()*@</div>
                                }
                                @*@foreach (var dayOfWeek in Enum.GetValues(typeof(DayOfWeek)))
                        {
                        <div class="week-day">@dayOfWeek.ToString()</div>
                        }*@
                            </div>
                            <div class="month-days grid">
                                @{
                                    DateTimeOffset firstDayOfMonth = new DateTimeOffset(year, month, 1, 0, 0, 0, TimeSpan.Zero);
                                    int daysInMonth = DateTime.DaysInMonth(year, month);
                                    int daysInLastMonth = DateTime.DaysInMonth(year, month == 1 ? 12 : month - 1);
                                    int firstDayOfWeek = (int)firstDayOfMonth.DayOfWeek;
                                    int lastDayOfWeek = (int)new DateTimeOffset(year, month, daysInMonth, 0, 0, 0, TimeSpan.Zero).DayOfWeek;
                                    int rows = (firstDayOfWeek + daysInMonth + (6 - lastDayOfWeek)) / 7;
                                    int day = 1 - firstDayOfWeek;
                                }
                                @for (int row = 0; row < rows; row++)
                                {
                                    @for (int col = 0; col < 7; col++)
                                    {
                                        if (day >= 1 && day <= daysInMonth)
                                        {
                                            DateTime currentDate = new DateTime(year, month, day);
                                            <div class="month-day" id="@currentDate.ToString("yyyy-MM-dd")">
                                                @day
                                                @foreach (var item in @ViewBag.Events)
                                                {
                                                    if (item.Begin.ToString("yyyy-MM-dd") == currentDate.ToString("yyyy-MM-dd"))
                                                    {
                                                        <div class="event">
                                                            <input type="hidden" value="@item.Id">
                                                            <p>@item.EventName</p>
                                                        </div>
                                                    }
                                                }
                                                @foreach (var item in @ViewBag.Tasks)
                                                {
                                                    if (item.Begin.ToString("yyyy-MM-dd") == currentDate.ToString("yyyy-MM-dd"))
                                                    {
                                                        <div class="event yellow">
                                                            <input type="hidden" value="@item.Id">
                                                            <p>@item.TaskName</p>
                                                        </div>
                                                    }
                                                }
                                                @foreach (var item in @ViewBag.Measures)
                                                {
                                                    if (item.Begin.ToString("yyyy-MM-dd") == currentDate.ToString("yyyy-MM-dd"))
                                                    {
                                                        <div class="event red">
                                                            <input type="hidden" value="@item.Id">
                                                            <p>@item.MeasureName</p>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        }
                                        else if (day < 1)
                                        {
                                            int prevMonthDay = daysInLastMonth + day;
                                            DateTime prevMonthDate = new DateTime(month == 1 ? year - 1 : year, month == 1 ? 12 : month - 1, prevMonthDay);
                                            <div class="month-day prev-month">
                                                @prevMonthDate.Day
                                            </div>
                                        }
                                        else
                                        {
                                            int nextMonthDay = day - daysInMonth;
                                            DateTime nextMonthDate = new DateTime(month == 12 ? year + 1 : year, month == 12 ? 1 : month + 1, nextMonthDay);
                                            <div class="month-day next-month">
                                                @nextMonthDate.Day
                                            </div>
                                        }
                                        day++;
                                    }
                                }
                            </div>
                        </div>
                    </div>
                }
            </div>
        }

    </div>




